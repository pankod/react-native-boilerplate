{
    "name": "ReactNativeBoilerplate",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "react-native start",
        "test": "jest",
        "lint": "eslint './{scenes,src}/**/*.{ts,tsx}'",
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "format": "npm run lint -- --fix",
        "pretty": "prettier --write --config .prettierrc.js ./{scenes,src}/**/*.{ts,tsx}",
        "clear": "watchman watch-del-all && rm -rf /tmp/metro-bundler-cache-* && rm -rf /tmp/haste-map-react-native-packager-* && react-native start --reset-cache"
    },
    "dependencies": {
        "@react-native-community/async-storage": "^1.6.1",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.14.0",
        "i18next": "^17.0.14",
        "isomorphic-fetch": "^2.2.1",
        "nock": "^11.3.4",
        "query-string": "^6.8.3",
        "react": "16.8.6",
        "react-i18next": "^10.12.4",
        "react-native": "0.60.5",
        "react-native-bootsplash": "^1.0.1",
        "react-native-code-push": "^5.7.0",
        "react-native-gesture-handler": "^1.4.1",
        "react-native-reanimated": "^1.2.0",
        "react-native-screens": "^2.0.0-alpha.3",
        "react-navigation": "^4.0.2",
        "react-navigation-stack": "^1.5.4",
        "react-redux": "^7.1.1",
        "redux": "^4.0.4",
        "redux-logger": "^3.0.6",
        "redux-persist": "^6.0.0",
        "redux-thunk": "^2.3.0",
        "styled-components": "^4.3.2"
    },
    "devDependencies": {
        "@babel/core": "7.6.0",
        "@babel/runtime": "7.6.0",
        "@react-native-community/eslint-config": "0.0.3",
        "@types/enzyme": "^3.10.3",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/jest": "24.0.18",
        "@types/jsdom": "^12.2.4",
        "@types/node": "^12.7.5",
        "@types/react": "16.9.2",
        "@types/react-native": "0.60.13",
        "@types/react-native-dotenv": "^0.2.0",
        "@types/react-redux": "^7.1.2",
        "@types/react-test-renderer": "16.9.0",
        "@types/redux-logger": "^3.0.7",
        "@types/redux-mock-store": "^1.0.1",
        "@types/styled-components": "^4.1.19",
        "@typescript-eslint/eslint-plugin": "^2.2.0",
        "babel-jest": "24.9.0",
        "babel-plugin-module-resolver": "^3.2.0",
        "enzyme-to-json": "^3.4.0",
        "eslint": "^5.0.0",
        "eslint-config-airbnb-typescript": "^4.0.1",
        "eslint-config-prettier": "^6.3.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.14.3",
        "jest": "24.9.0",
        "jest-styled-components": "^6.3.3",
        "jsdom": "^15.1.1",
        "metro-react-native-babel-preset": "^0.54.1",
        "react-dom": "^16.9.0",
        "react-native-dotenv": "^0.2.0",
        "react-test-renderer": "16.8.6",
        "redux-mock-store": "^1.5.3",
        "typescript": "3.6.2"
    },
    "jest": {
        "preset": "react-native"
